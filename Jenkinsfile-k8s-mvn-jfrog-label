pipeline {
  agent { label "mvn-jfrog-simple" }
  stages {
    stage("build") {
      steps {
        container("maven") {
          sh """
            mvn --version
          """
        }            
      }
    }
    stage("deploy") {
      steps {
        container("jfrog") {
          sh """
            jfrog --version
          """
        }
      }
    }
  }
}
